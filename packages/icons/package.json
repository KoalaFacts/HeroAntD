{
  "name": "@hero-antd/icons",
  "version": "6.1.4",
  "description": "Ant Design icons as Web Components - tree-shakeable individual components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/hero-antd-icons/hero-antd-icons.esm.js",
  "exports": {
    ".": {
      "import": "./dist/hero-antd-icons/hero-antd-icons.esm.js",
      "require": "./dist/hero-antd-icons/hero-antd-icons.cjs.js"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs"
    },
    "./outlined": {
      "import": "./dist/components/outlined.js"
    },
    "./filled": {
      "import": "./dist/components/filled.js"
    },
    "./twotone": {
      "import": "./dist/components/twotone.js"
    }
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "dev": "bun run generate:icons && stencil build --dev --watch",
    "build": "bun run generate:icons && stencil build",
    "generate:icons": "bun run scripts/generate-icons.ts"
  },
  "dependencies": {
    "@ant-design/icons-svg": "^4.4.2"
  },
  "devDependencies": {
    "@stencil/core": "^4.41.0",
    "@types/bun": "^1.3.5"
  },
  "license": "MIT"
}
