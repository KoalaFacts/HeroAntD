{
  "name": "@hero-antd/core",
  "version": "6.1.4",
  "description": "Web Components UI library for Ant Design V6",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/hero-antd/hero-antd.esm.js",
  "exports": {
    ".": {
      "import": "./dist/hero-antd/hero-antd.esm.js",
      "require": "./dist/hero-antd/hero-antd.cjs.js"
    },
    "./styles": "./dist/hero-antd/hero-antd.css",
    "./styles.css": "./dist/hero-antd/hero-antd.css",
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "dev": "stencil build --dev --watch --serve",
    "build": "stencil build",
    "test": "stencil test --spec",
    "test:watch": "stencil test --spec --watchAll",
    "test:e2e": "stencil test --e2e",
    "generate": "stencil generate",
    "scaffold": "bun run scripts/scaffold.ts"
  },
  "dependencies": {
    "@hero-antd/icons": "workspace:*",
    "@hero-antd/tokens": "workspace:*"
  },
  "devDependencies": {
    "@stencil/core": "^4.41.0",
    "@stencil/sass": "^3.2.3",
    "@types/bun": "^1.3.5",
    "rollup": "^4.55.1"
  },
  "license": "MIT"
}
